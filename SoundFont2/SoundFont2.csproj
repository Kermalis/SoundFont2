<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<OutputType>Library</OutputType>
		<LangVersion>latest</LangVersion>
		<RootNamespace>Kermalis.SoundFont2</RootNamespace>
		<Nullable>enable</Nullable>
		<OutputPath>..\Build</OutputPath>

		<Authors>Kermalis</Authors>
		<Copyright>Kermalis</Copyright>
		<Product>SoundFont2</Product>
		<Title>SoundFont2</Title>
		<AssemblyName>SoundFont2</AssemblyName>
		<Version>1.1.0</Version>

		<Configurations>Release</Configurations>
		<GenerateSerializationAssemblies>Auto</GenerateSerializationAssemblies>
		<DebugType>none</DebugType>
		<DebugSymbols>false</DebugSymbols>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="EndianBinaryIO" Version="2.0.0" />
	</ItemGroup>

</Project>
